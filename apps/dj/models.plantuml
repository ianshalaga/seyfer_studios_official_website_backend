@startuml dj

' Enums

Enum SongArtistState {
    YES = "Allowed"
    NO = "Not Allowed"
    NEW = "New"
}

' Classes

class Song {
    - title: String
    - state: SongArtistState
}

class Artist {
    - name: String
    - state: SongArtistState
}

' Relationships

Song "1..*" -- "1..*" Artist

@enduml